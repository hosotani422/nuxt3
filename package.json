{
  "name": "nuxt3",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "ui": "storybook dev -p 6006",
    "build:ui": "storybook build",
    "run:ui": "http-server storybook-static --port 6006",
    "lint:format": "prettier --check --config prettier.config.js .",
    "lint:style": "stylelint --config stylelint.config.mjs **/*.css",
    "lint:script": "eslint --config .eslintrc.cjs **/*.{js,jsx,ts,tsx,vue}",
    "fix:format": "prettier --write --config prettier.config.js .",
    "fix:style": "stylelint --fix --config stylelint.config.mjs **/*.css",
    "fix:script": "eslint --fix --config .eslintrc.cjs **/*.{js,jsx,ts,tsx,vue}",
    "test:ui": "test-storybook --maxWorkers=1 --no-cache --url http://localhost:6006/",
    "test:ui.ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"pnpm ui\" \"wait-on tcp:6006 && pnpm test:ui\"",
    "test:ut": "vitest --config test/vitest/vitest.config.ts",
    "test:ut.c": "vitest --run --coverage --config test/vitest/vitest.config.ts",
    "test:it": "playwright test --config test/playwright/playwright.config.ts",
    "test:it.testcafe": "testcafe --config-file test/testcafe/.testcaferc.cjs",
    "test:it.cypress": "cypress run --browser chrome --config-file test/cypress/cypress.config.ts",
    "prepare": "husky install && playwright install"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "^8.3.0",
    "@pinia/nuxt": "^0.5.1",
    "@playwright/test": "^1.42.1",
    "@storybook-vue/nuxt": "^0.2.6",
    "@storybook/addon-essentials": "^8.0.5",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-vite": "^8.0.5",
    "@storybook/test-runner": "^0.17.0",
    "@storybook/vue3-vite": "^8.0.5",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.3",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.4.0",
    "@vue/test-utils": "^2.4.5",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cypress": "^13.7.2",
    "dayjs": "^1.11.10",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "flush-promises": "^1.0.2",
    "happy-dom": "^14.3.10",
    "http-server": "^14.1.1",
    "husky": "^9.0.11",
    "lodash": "^4.17.21",
    "nuxt": "^3.11.1",
    "pinia": "^2.1.7",
    "postcss": "^8.4.38",
    "postcss-syntax": "^0.36.2",
    "prettier": "^3.2.5",
    "storybook": "^8.0.5",
    "stylelint": "^16.3.1",
    "stylelint-config-recommended": "^14.0.0",
    "tailwindcss": "^3.4.3",
    "testcafe": "^3.5.0",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vitest": "^1.4.0",
    "wait-on": "^7.2.0"
  },
  "resolutions": {
    "@typescript-eslint/typescript-estree": "^7.5.0",
    "@types/mime": "3.0.4"
  }
}
