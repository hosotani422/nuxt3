{
  "name": "nuxt3",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "ui": "storybook dev -p 9009 -c test/storybook",
    "lint:format": "prettier --check --config prettier.config.js .",
    "lint:style": "stylelint --config stylelint.config.mjs **/*.css",
    "lint:script": "eslint --config .eslintrc.cjs **/*.{js,jsx,ts,tsx,vue}",
    "fix:format": "prettier --write --config prettier.config.js .",
    "fix:style": "stylelint --fix --config stylelint.config.mjs **/*.css",
    "fix:script": "eslint --fix --config .eslintrc.cjs **/*.{js,jsx,ts,tsx,vue}",
    "test:unit": "vitest --config test/vitest/vitest.config.ts",
    "test:e2e": "playwright test --config test/playwright/playwright.config.ts",
    "test:e2e.testcafe": "testcafe --config-file test/testcafe/.testcaferc.cjs",
    "test:e2e.cypress": "cypress run --browser firefox --config-file test/cypress/cypress.config.ts"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@pinia/nuxt": "^0.4.11",
    "@playwright/test": "^1.37.1",
    "@storybook/addon-essentials": "^7.3.1",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/vue3-vite": "^7.3.1",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.5.0",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vitest/coverage-v8": "^0.34.3",
    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "^10.4.15",
    "cypress": "^12.17.4",
    "dayjs": "^1.11.9",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "flush-promises": "^1.0.2",
    "lodash": "^4.17.21",
    "nuxt": "^3.6.5",
    "pinia": "^2.1.4",
    "postcss": "^8.4.28",
    "postcss-syntax": "^0.36.2",
    "prettier": "^3.0.3",
    "storybook": "^7.3.1",
    "stylelint": "^15.10.2",
    "stylelint-config-recommended": "^13.0.0",
    "tailwindcss": "^3.3.3",
    "testcafe": "^3.2.0",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vitest": "^0.34.2"
  }
}
